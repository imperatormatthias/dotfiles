# Audio Keys

"amixer -D default sset Master 5%-"
  XF86AudioLowerVolume
"amixer -D default sset Master 5%+"
  XF86AudioRaiseVolume
"amixer -D default sset Master toggle"
  XF86AudioMute

"$HOME/scripts/mpc_toggle"
  XF86AudioPlay
"mpc next"
  XF86AudioNext
"mpc prev"
  XF86AudioPrev

# Brightness Keys

"xbacklight -dec 5"
  XF86MonBrightnessDown
"xbacklight -inc 5"
  XF86MonBrightnessUp

"$HOME/Programming/i3lockblur/i3lockblur.sh"
  XF86ScreenSaver

# External Display

"$HOME/scripts/monitor"
  XF86Display
